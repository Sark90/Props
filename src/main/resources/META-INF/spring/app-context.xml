<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--mvc-->

<!--    <context:annotation-config />
    <context:component-scan base-package="levelup.print" />-->

    <!--init-method="init" destroy-method="destroy"-->
    <bean class="org.springframework.beans.factory.config.CustomEditorConfigurer">
          <property name="customEditors">
              <map>
                  <entry key="levelup.Date" value="levelup.DateEditor">
                      <!--<bean id="dateEditor" class="levelup.DateEditor"/>-->
                  </entry>
              </map>
          </property>
    </bean>

    <bean id="user" class="levelup.User">
        <property name="birthday" value="01-01-1970"/>
        <property name="name" value="James"/>
        <property name="surname" value="Bond"/>
    </bean>

<!--    <bean id="birthday" class="levelup.Date">
        <property name="year"/>
        <property name="month"/>
        <property name="day"/>
    </bean>-->

</beans>